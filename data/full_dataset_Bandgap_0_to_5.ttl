@prefix ex: <http://example.org/mse#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:CrystalStructure a rdfs:Class .

ex:Material a rdfs:Class .

ex:Property a rdfs:Class .

ex:GaN ex:hasBandGap "3.4"^^xsd:float .

ex:hasBandGap a rdf:Property ;
    rdfs:domain ex:Material ;
    rdfs:range xsd:float .

ex:hasCentrosymmetric a rdf:Property ;
    rdfs:domain ex:Material ;
    rdfs:range xsd:boolean .

ex:hasCrystalSystem a rdf:Property ;
    rdfs:domain ex:Material ;
    rdfs:range xsd:string .

ex:hasExternalId a rdf:Property ;
    rdfs:domain ex:Material ;
    rdfs:range xsd:string .

ex:hasFormula a rdf:Property ;
    rdfs:domain ex:Material ;
    rdfs:range xsd:string .

